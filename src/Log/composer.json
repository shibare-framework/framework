{
  "$schema": "https://getcomposer.org/schema.json",
  "name": "shibare/log",
  "description": "S.S.S. Shibare Framework - Log Component",
  "type": "library",
  "keywords": [
    "shibare",
    "log",
    "psr3"
  ],
  "homepage": "https://shibare-framework.github.io/",
  "readme": "README.md",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Masaru Yamagishi",
      "email": "akai_inu@live.jp",
      "homepage": "https://cafe-capy.net",
      "role": "Author"
    }
  ],
  "support": {
    "docs": "https://shibare-framework.github.io/",
    "issues": "https://github.com/shibare-framework/framework/issues",
    "source": "https://github.com/shibare-framework/framework/"
  },
  "provide": {
    "psr/log-implementation": "3.0.0"
  },
  "require": {
    "php": "~8.2.0|~8.3.0",
    "psr/log": "^2.0.0|^3.0.0"
  },
  "require-dev": {
    "monolog/monolog": "^3.7.0"
  },
  "suggest": {
    "monolog/monolog": "^3.0 To use MonologWriter"
  },
  "autoload": {
    "psr-4": {
      "Shibare\\Log\\": ""
    },
    "exclude-from-classmap": [
      "Tests/"
    ]
  },
  "scripts": {},
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
