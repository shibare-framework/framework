{
  "$schema": "https://getcomposer.org/schema.json",
  "name": "shibare/idl-openapi",
  "description": "S.S.S. Shibare Framework - IDL OpenAPI Component",
  "type": "library",
  "keywords": [
    "shibare",
    "idl",
    "openapi",
    "swagger"
  ],
  "homepage": "https://shibare-framework.github.io/",
  "readme": "README.md",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Masaru Yamagishi",
      "email": "m.yamagishi90+git@gmail.com",
      "homepage": "https://cafe-capy.net",
      "role": "Author"
    }
  ],
  "support": {
    "docs": "https://shibare-framework.github.io/",
    "issues": "https://github.com/shibare-framework/framework/issues",
    "source": "https://github.com/shibare-framework/framework/"
  },
  "require": {
    "php": "~8.2.0|~8.3.0|~8.4.0"
  },
  "require-dev": {},
  "suggest": {},
  "autoload": {
    "psr-4": {
      "Shibare\\IdlOpenApi\\": ""
    },
    "exclude-from-classmap": [
      "*Test.php"
    ]
  },
  "scripts": {
    "test": "phpunit *Test.php"
  },
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
