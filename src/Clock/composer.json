{
  "$schema": "https://getcomposer.org/schema.json",
  "name": "shibare/clock",
  "description": "S.S.S. Shibare Framework - Clock Component",
  "type": "library",
  "keywords": [
    "shibare",
    "clock",
    "time",
    "psr20"
  ],
  "homepage": "https://shibare-framework.github.io/",
  "readme": "README.md",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Masaru Yamagishi",
      "email": "m.yamagishi90+git@gmail.com",
      "homepage": "https://cafe-capy.net",
      "role": "Author"
    }
  ],
  "support": {
    "docs": "https://shibare-framework.github.io/",
    "issues": "https://github.com/shibare-framework/framework/issues",
    "source": "https://github.com/shibare-framework/framework/"
  },
  "provide": {
    "psr/clock-implementation": "1.0"
  },
  "require": {
    "php": "~8.2.0|~8.3.0",
    "psr/clock": "^1.0.0"
  },
  "require-dev": {
    "cakephp/chronos": "^3.0.0",
    "nesbot/carbon": "^3.0.0"
  },
  "suggest": {
    "nesbot/carbon": "^3.0 To use ::AsCarbonImmutable method",
    "cakephp/chronos": "^3.0 To use ::AsChronos method"
  },
  "autoload": {
    "psr-4": {
      "Shibare\\Clock\\": ""
    },
    "files": [
      "functions.php"
    ],
    "exclude-from-classmap": [
      "Tests/"
    ]
  },
  "scripts": {},
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
